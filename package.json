{
  "name": "fanhehe.backend",
  "version": "1.0.0",
  "description": "a web restful api server for fanhehe.com",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon -e js ./bin/www.js",
    "deploy": "cross-env NODE_ENV=product ./node_modules/.bin/pm2 start bin/www.js -i 0 --name backend --no-daemon",
    "compiler": "./node_modules/.bin/tsc --watch",
    "clean": "./node_modules/.bin/gulp clean",
    "dist": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/mocha ./test --recursive -r babel-core/register -r test/setup",
    "test:dev": "/node_modules/.bin/mocha ./test --recursive -r babel-core/register -r test/setup -w"
  },
  "keywords": [
    "backend",
    "fanhehe.com",
    "restful",
    "api"
  ],
  "author": "fanhehe",
  "license": "ISC",
  "dependencies": {
    "es6-promise": "^4.0.5",
    "events": "^1.1.1",
    "fs": "^0.0.1-security",
    "ioredis": "^2.4.3",
    "keygrip": "^1.0.1",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^2.3.0",
    "koa-json": "^1.1.3",
    "koa-logger": "^1.3.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.0.1",
    "koa-session2": "^1.0.8",
    "morgan": "^1.7.0",
    "mysql": "^2.12.0",
    "object-assign": "^4.1.0",
    "path": "^0.12.7",
    "sequelize": "^3.27.0"
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/koa": "^2.0.35",
    "@types/node": "^6.0.52",
    "babel-core": "^6.18.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.0",
    "typescript": "^2.2.0-dev.20161218"
  }
}
